@model Person

<div class="row">
    <div class="col2">@Model.PersonId</div>
    <div class="col2">@Model.Name</div>
    <div class="col2">@Model.Phone</div>
    <div class="col2">@Model.City</div>
    <div class="col2"><a asp-action="Delete" asp-controller="People" asp-route-id=@Model.PersonId>Delete</a></div>
</div>